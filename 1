#include <iostream>

using namespace std;
class Rectangle
{
 public:
  float width;
  float height;
  float Area()
  {
   float S;
   S=width*height;
   return S;
  }
  float Perimetur()
  {
    return (width+height)*2;
  }
  void info()
  {
   cout<<"W:"<<width<<endl<<"H:"<<height<<endl;
   cout<<"S:"<<Area()<<endl<<"P:"<<Perimetur()<<endl;
  }
};
int main()
{
    Rectangle r1;
    r1.height=5;
    r1.width=45;
    cout<<r1.Area()<<endl;
    cout<<r1.Perimetur()<<endl;
    r1.info();

}
